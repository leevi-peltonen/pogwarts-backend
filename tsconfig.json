{
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "./dist"
  },
    "include": [
        "index.ts",
        "index.ts",
        "models",
        "dbmodels",
        "utils",
        "controllers"
    ],
    "exclude": [
      "node_modules"
    ]
}